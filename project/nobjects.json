{
  "fps": 30,
  "duration": 50,
  "allow_overlap": false,
  "step_size": 2,
  "media_root": "../resources",
  "background_path": "../resources/street.jpg",
  "max_cores": 16,
  "use_cache": true,
  "dimension": {
    "x": 600,
    "y": 600
  },
  "transformers": [
    {
      "type": "resize_transformer",
      "ratio": 0.9
    }
  ],
  "fuzzer": {
    "type": "targeted_fuzzer",
    "transformers": [
      {
        "type": "rotate_transformer",
        "angle": 10
      }
    ]
  },
  "num_video_copies": 1,
  "compositor": {
    "type": "grid_compositor"
  },
  "object_class": [
    "car", "person", "motorbike", "bicycle", "bus", "truck"
  ],
  "object_distribution": {
    "type": "normal",
    "mean": 5,
    "std": 3
  },
  "time_distribution": {
    "type": "normal",
    "mean": 5,
    "std": 2,
    "fix_value": 1
  }
}
