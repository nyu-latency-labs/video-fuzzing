{
    "fps": 25,
    "duration": 40,
    "allow_overlap": false,
    "step_size": 2,
    "media_root": "../resources",
    "background_path": "../resources/street.jpg",
    "max_cores": 1,
    "use_cache" : false,
    "dimension": {
        "x": 400, "y": 400
    },
    "transformers": [
        {
            "type": "resize_transformer",
            "ratio": 0.9
        }
    ],
    "fuzzer": {
        "type": "experiment_fuzzer",
        "transformers": [
            {
                "type": "rotate_transformer",
                "angle": 45
            },
            {
                "type": "resize_transformer",
                "ratio": 0.9
            }
        ],
        "num_videos": 1
    },
    "compositor": {
        "type": "grid_compositor"
    },
    "object_class": ["car", "person", "motorcycle"],
    "object_distribution": {
        "type": "normal",
        "mean": 2,
        "std": 1
    },
    "time_distribution": {
        "type": "linear",
        "value": 3
    }
}