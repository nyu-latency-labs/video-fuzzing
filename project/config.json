{
  "fps": 5,
  "duration":5,
  "allow_overlap": false,
  "step_size": 2,
  "media_root": "../resources",
  "background_path": "../resources/street.jpg",
  "max_cores": 8,
  "use_cache": true,
  "dimension": {
    "x": 400,
    "y": 400
  },
  "fuzzer": {
    "type": "randomized_fuzzer",
    "transformers": [
      {
        "type": "resize_transformer",
        "dimension": {
          "x": 200,
          "y": 200
        }
      }
    ]
  },
  "num_video_copies": 1,
  "compositor": {
    "type": "grid_compositor"
  },
  "object_class": [
    "car", "person", "bench"
  ],
  "object_distribution": {
    "type": "alpine",
    "multiplier": 3.5,
    "downscale": 5
  },
  "time_distribution": {
    "type": "normal",
    "mean": 5,
    "std": 2
  },
  "model": "yolo",
  "model_confidence": 0.25
}